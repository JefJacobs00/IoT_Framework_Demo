@prefix : <http://www.semanticweb.org/jef/ontologies/2022/10/demo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://www.semanticweb.org/jef/ontologies/2022/10/demo> a owl:Ontology .

:IpAddress1 a :IpAddress ;
    :ipv4 "192.168.0.106" .

:Port1 a :Port ;
    :portNumber "22" ;
    :portService :Service1 .

:accountPassword a owl:ObjectProperty ;
    rdfs:domain :Account ;
    rdfs:range :Password .

:accountUsername a owl:DatatypeProperty ;
    rdfs:domain :Account .

:deviceAccount a owl:ObjectProperty ;
    rdfs:domain :Device ;
    rdfs:range :Account .

:deviceAddress a owl:ObjectProperty ;
    rdfs:domain :Device ;
    rdfs:range :IpAddress .

:devicePort a owl:ObjectProperty ;
    rdfs:domain :Device ;
    rdfs:range :Port .

:hashFunction a owl:DatatypeProperty ;
    rdfs:domain :Hash .

:hashValue a owl:DatatypeProperty ;
    rdfs:domain :Hash .

:ipv4 a owl:DatatypeProperty ;
    rdfs:domain :IpAddress .

:passwordCleartext a owl:DatatypeProperty ;
    rdfs:domain :Password .

:passwordHash a owl:ObjectProperty ;
    rdfs:domain :Password ;
    rdfs:range :Hash .

:portNumber a owl:DatatypeProperty ;
    rdfs:domain :Port .

:portService a owl:ObjectProperty ;
    rdfs:domain :Port ;
    rdfs:range :Service .

:portStatus a owl:DatatypeProperty ;
    rdfs:domain :Port .

:serviceAccount a owl:ObjectProperty ;
    rdfs:domain :Service ;
    rdfs:range :Account .

:serviceName a owl:DatatypeProperty ;
    rdfs:domain :Service .

:webservicePage a owl:ObjectProperty ;
    rdfs:domain :WebService ;
    rdfs:range :Webpage .

:Account1 a :Account ;
    :accountPassword :Password1 ;
    :accountUsername "admin" .

:Password1 a :Password ;
    :passwordCleartext "Password" .

:Service1 a :Service ;
    :serviceAccount :Account1 ;
    :serviceName "ssh" .

:WebService a owl:Class ;
    rdfs:subClassOf :Service .

:Webpage a owl:Class .

:Device a owl:Class .

:Hash a owl:Class .

:IpAddress a owl:Class .

:Password a owl:Class .

:Account a owl:Class .

:Port a owl:Class .

:Service a owl:Class .

