{
  "Tool":{
  "name": "nmap",
  "version": "",
    "Profile": {
      "fast_scan": {
        "Requirement": {"ip": "_"},
        "Result": ["port", "service"],
        "Command": "nmap -F <ip>"
      },
      "full_scan": {
        "Requirement": {"ip": "Ip", "port": "Port", "service": "ssh"},
        "Result": ["port", "service"],
        "Command": "nmap -F -sC -sV -p- <ip>"
      }
    }
  }
}