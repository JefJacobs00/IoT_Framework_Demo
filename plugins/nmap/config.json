{
  "Tool":{
  "toolName": "nmap",
  "version": "",
    "Profile": {
      "fast_scan": {
        "Requirement": {"ipv4": "Ip", "\\+executed": ["full_scan", "_"]},
        "Parameter": {"ipv4": "Ip"},
        "Result": ["port", "service"],
        "Command": "nmap -F <ipv4>"
      },
      "full_scan": {
        "Requirement": {"ipv4": "Ip"},
        "Parameter": {"ipv4": "Ip"},
        "Result": ["port", "service"],
        "Command": "nmap -sC -sV -p- <ipv4>"
      },
      "udp_scan": {
        "Requirement": {"ipv4": "Ip"},
        "Parameter": {"ipv4": "Ip"},
        "Result": ["port", "service"],
        "Command": "sudo nmap -sU <ipv4>"
      }
    }
  }
}